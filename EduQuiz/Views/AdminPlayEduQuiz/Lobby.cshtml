@{
    Layout = "_LayoutPlay";
    ViewBag.Title = "Eduquiz! Game";
}
<main class="lobbystyles__PageWrapper">
    <div id="header-title" class="lobbystyles__GetReadyToJoinWrapper">
        <div class="lobbystyles__GetReadyToJoin">
