<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
	<link href="https://fonts.googleapis.com/css?family=Poppins:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">
	<!-- Bootstrap CSS -->
	<link href="~/src/css/bootstrap.min.css" rel="stylesheet" />
	<!-- Medipro CSS -->
    <link href="~/src/css/normalize.css" rel="stylesheet" />
	<link rel="stylesheet" href="~/src/css/responsive.css">
    <link href="~/src/css/auth.css" rel="stylesheet" />
    <link href="~/src/css/toast.css" rel="stylesheet" />
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" />
	<!-- Recapcha v2 -->
	<script src="https://www.google.com/recaptcha/api.js" async defer></script>
</head>
<body>
	<div class="root">
		<div class="loading" style="display:none">Loading&#8230;</div>
		<div class="topbar">
			<header class="header-nav">
				<!-- Topbar -->
				<div class="topbar">
					<div class="row">
						<div class="col-lg-6 col-md-5 col-12">
							<div class="logo">
								<a href="/"><img style="height:48px" src="~/src/img/logomain.png" alt="#"></a>
							</div>
						</div>
						<div class="col-lg-6 col-md-7 col-12">
						</div>
					</div>
				</div>
			</header>
		</div>

		<div class="layout-page-content">
			<div class="wrapper-content">
				<span class="circle-box bg-icon">
					<svg viewBox="0 0 32 32" focusable="false" stroke="none" stroke-width="0" aria-labelledby="label-dd9f731c-1691-40b8-9220-bc6dac149444" aria-hidden="true" class="icon__Svg-sc-xvsbpg-1 ipIYNE"><title id="label-dd9f731c-1691-40b8-9220-bc6dac149444">Icône</title><path d="M16,27 C9.92486775,27 5,22.0751322 5,16 C5,9.92486775 9.92486775,5 16,5 C22.0751322,5 27,9.92486775 27,16 C27,22.0751322 22.0751322,27 16,27 Z" style="fill: rgb(204, 204, 204);"></path></svg>
				</span>
				<span class="rectangle-box bg-icon">
					<svg viewBox="0 0 32 32" focusable="false" stroke="none" stroke-width="0" aria-labelledby="label-b0f66aa4-e070-47c1-a4ea-feacec0fdea3" aria-hidden="true" class="icon__Svg-sc-xvsbpg-1 ipIYNE"><title id="label-b0f66aa4-e070-47c1-a4ea-feacec0fdea3">Icône</title><path d="M7,7 L25,7 L25,25 L7,25 L7,7 Z" style="fill: rgb(204, 204, 204);"></path></svg>
				</span>
			@RenderBody()
			</div>
		</div>
	
			
	</div>
	<section id="toast" class="info off">
		<div id="icon-wrapper">
			<div id="icon"></div>
		</div>
		<div id="toast-message">
			<h4 id="toast-title"></h4>
			<p id="toast-description"></p>
		</div>
		<button id="toast-close"></button>
		<div id="timer"></div>
	</section>

	<!-- Popper JS -->
	<script src="@("https://unpkg.com/@popperjs/core@2")"></script>
	<script src="https://unpkg.com/tippy.js@6"></script>

    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="~/src/js/toast.js"></script>
	<!-- jquery Migrate JS -->
	@RenderSection("scripts", required: false)
</body>
</html>
